{
  "manifest_version": 3,
  "name": "Task List",
  "version": "1.0.0",
  "description": "Helps you get things done!",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "Task List",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["storage"],
  "options_page": "options/options.html"
}
